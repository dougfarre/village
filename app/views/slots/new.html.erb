<%= form_for(@slot) do |f| %>
<h2> New Slot for 
	<% if !params[:current_date].blank? %>
 		<%= label_tag :start_date, Date.parse(params[:current_date]).strftime("%A, %b %d %Y") %>
	<% else %>
   	<%= f.date_select :start_date %>
	<% end %>
</h2>
<div style="margin-right:50px;">
<%= render 'form' %>
</div>
<br/>
<%= link_to 'Back', :back %>
<% end %>
